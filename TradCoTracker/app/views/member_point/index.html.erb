<h1>MemberPoint#index</h1>
<p>Find me in app/views/member_point/index.html.erb</p>
<div class="points index">
  <h2>Subjects</h2>
  <table class="listing" summary="Subject list">
    <tr class="header">
      <th>User</th>
      <th>Events</th>
      <th>Points</th>
    </tr>
    <% @points.each do |point| %>
    <tr>
      <td><%= point.userID %></td>
      <td><%= point.events %></td>
      <td><%= point.points %></td>
    </tr>
    <% end %>
  </table>
</div>